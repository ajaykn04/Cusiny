{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",   // Adjust this for any API endpoints
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",       // Catch-all route
            "dest": "/"
        }
    ]
}
