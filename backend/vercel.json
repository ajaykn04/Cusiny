{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/signin",
            "dest": "/"
        },
        {
            "src": "/admindash",
            "dest": "/"
        },
        {
            "src": "/userdash",
            "dest": "/"
        },
        {
            "src": "/recipe/add",
            "dest": "/"
        },
        {
            "src": "/profile",
            "dest": "/"
        },
        {
            "src": "/user/recipes",
            "dest": "/"
        },
        {
            "src": "/recipes",
            "dest": "/"
        },
        {
            "src": "/detrecipe",
            "dest": "/"
        },
        {
            "src": "/detrecipeadmin",
            "dest": "/"
        },
        {
            "src": "/admin/users",
            "dest": "/"
        },
        {
            "src": "/admin/recipes",
            "dest": "/"
        }
        
    ]
}
